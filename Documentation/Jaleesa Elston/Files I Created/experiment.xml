    <!-- The default value is "seconds" if it is left unspecified. -->
	<da>
      <rewardType> orange </rewardType>
	  <precision> 4 </precision>
      <minReward> 0.5 </minReward>
      <maxReward> 10 </maxReward>
      <maxTime units="minutes"> 2 </maxTime>
      <responseDelay> 1 </responseDelay>
      <numTrials> 3 </numTrials>
	  <daInstructions> Instructions for Decreasing Adjustment go here! </daInstructions>
	  <soonerTime> 0 </soonerTime>
	  <laterTime> 0 </laterTime>
	  <doubleDelay> 0 </doubleDelay>
	  <intertrialInterval> 2 </intertrialInterval>
	  <question> Would you rather </question>
    </da>
    <dl>
      <rewardType> apples </rewardType>
      <minReward> 0.5 </minReward>
      <maxReward> 10 </maxReward>
      <maxTime units="minutes"> 2 </maxTime>
      <responseDelay units="seconds"> 0 </responseDelay>
	  <dlInstructions> Instructions for Double Limit go here! </dlInstructions>
	  <soonerTime> 0 </soonerTime>
	  <laterTime> 0 </laterTime>
	  <doubleDelay> 0 </doubleDelay>
	  <intertrialInterval> 3 </intertrialInterval>
	  <question> Would you rather </question>
	</dl>
    <mc>
      <rewardType> pesos </rewardType>
      <minReward> 0.5 </minReward>
      <maxReward> 10 </maxReward>
      <maxTime units="minutes"> 2 </maxTime>
      <responseDelay units="seconds"> 1 </responseDelay>
	  <distribution> Exponential </distribution>
      <scenario>
        <option>
          <reward> 0.01 </reward>
          <time> 0 </time>
        </option>
        <option>
          <reward> 0.5 </reward>
          <time units="seconds"> 30 </time>
        </option>
        <option>
          <reward> 1 </reward>
